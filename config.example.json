{
  "database_file": "data/storage.db",
  "server": {
    "address": ":8037",
    "is_behind_proxy": false,
    "read_timeout": 5,
    "write_timeout": 10,
    "idle_timeout": 60
  },
  "ratelimiter": {
    "max_requests": 15,
    "in_seconds": 5
  },
  "caches": {
    "static_files": 86400,
    "fish_me": 604800,
    "pixels_limit": 604800
  }
}
